/**app.wxss**/
@import "./font/iconfont.wxss";

page {
  background: rgb(243, 243, 243);
  font-size: 28rpx;
}
.container {
  display: block;
} 

.theme-color {
  color: #4cb4e7;
}

.theme-background {
  background: #4cb4e7;
}

.theme-font {
  font-size: 28rpx;
}

.theme-h1 {
  font-size: 34rpx;
}

.theme-h2 {
  font-size: 32rpx;
}

button {
  background: transparent;
}
button::after{
  border: 0;
  background: transparent;
}

.overview {
    width: 626rpx;
    margin: 0 auto 30rpx;
    border-radius: 6rpx;
    background: #fff;
    padding: 0 30rpx;
}
.overview .item {
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1px solid #ccc;
    padding: 0 14rpx;
}

.overview .item:last-child{
    border: 0;
}

.no-do{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 50rpx;
  font-weight: 700;
  color: #999;
}